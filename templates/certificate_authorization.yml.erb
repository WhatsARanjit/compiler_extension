---
  <%= @console %>:
    role: read-write
<% if @ca != ( @console or @puppetdb ) -%>
  <%= @ca %>:
    role: read-write
<% end -%>
<% if @puppetdb != ( @console or @ca ) -%>
  <%= @puppetdb %>:
    role: read-write
<% end -%>
<% @compilers.each do |compiler| -%>
<% if compiler != ( @console or @ca or @puppetdb ) -%>
  <%= compiler %>:
    role: read-write
<% end -%>
<% end -%>
